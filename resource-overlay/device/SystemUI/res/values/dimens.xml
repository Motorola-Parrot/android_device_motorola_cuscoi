<resources>
    <!-- the padding on the start of the statusbar -->
    <dimen name="status_bar_padding_start">18.0dip</dimen>

    <!-- the padding on the end of the statusbar -->
    <dimen name="status_bar_padding_end">1.0dip</dimen>

    <!-- the padding on the top of the statusbar (usually 0) -->
    <dimen name="status_bar_padding_top">5.0dip</dimen>

    <!-- Height of the status bar header bar when on Keyguard -->
    <dimen name="status_bar_header_height_keyguard">@*android:dimen/status_bar_height</dimen>

    <!-- Margin on the left side of the carrier text on Keyguard -->
    <dimen name="keyguard_carrier_text_margin">16.0dip</dimen>

    <!-- Bottom margin size when fingerprint in display is active -->
    <dimen name="keyguard_indication_margin_bottom">16.0dip</dimen>

    <!--  Total minimum padding to enforce to ensure that the dot can always show  -->
    <dimen name="ongoing_appops_dot_min_padding">14dp</dimen>

  	<!-- Location on the screen of the center of the physical power button. -->
    <dimen name="physical_power_button_center_screen_location_y">990.0px</dimen>

    <!-- Location on the screen of the center of the physical volume up/down buttons. -->
    <dimen name="physical_volume_up_button_center_screen_location_y">594.0px</dimen>
    <dimen name="physical_volume_down_button_center_screen_location_y">749.0px</dimen>

    <!-- Microns/ums (1000 um = 1mm) per pixel for the given device. If unspecified, UI that
         relies on this value will not be sized correctly. -->
    <item format="float" name="pixel_pitch" type="dimen">56.47</item>
</resources>
